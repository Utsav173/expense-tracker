{
  "scripts": {
    "dev": "bun run --hot src/index.ts",
    "start": "bun run dist/index.js",
    "generate:build": "bun build ./src/index.ts --outdir ./dist --target bun",
    "db:create": "bash ./scripts/db-setup.sh",
    "db:stop": "docker-compose down",
    "db:clean": "docker-compose down -v",
    "db:pull": "bunx drizzle-kit pull",
    "db:push": "bunx drizzle-kit push",
    "db:generate": "bunx drizzle-kit generate",
    "db:studio": "bunx drizzle-kit studio",
    "db:check": "bunx drizzle-kit check",
    "seed": "bun run src/database/seed.ts",
    "reseed": "bun run src/database/reseed.ts",
    "format": "prettier . --write"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.2.14",
    "@hono/zod-validator": "^0.2.1",
    "@neondatabase/serverless": "^0.9.0",
    "@supercharge/promise-pool": "^3.2.0",
    "ai": "^4.3.13",
    "bcryptjs": "^3.0.2",
    "chalk": "^5.3.0",
    "chance": "^1.1.11",
    "chrono-node": "^2.8.0",
    "cli-progress": "^3.12.0",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.2.0",
    "drizzle-orm": "^0.38.2",
    "hono": "^4.2.3",
    "jimp": "^1.6.0",
    "lodash": "^4.17.21",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.13",
    "papaparse": "^5.5.2",
    "pg": "^8.11.5",
    "postgres": "^3.4.4",
    "xlsx": "^0.18.5",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/bun": "latest",
    "@types/chance": "^1.1.6",
    "@types/cli-progress": "^3.11.5",
    "@types/lodash": "^4.17.16",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
